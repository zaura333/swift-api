{
  "name": "swift-api",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"No test specified\"",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "nodemon src/index.ts",
    "restart-db": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate",
    "seed-db": "npx sequelize-cli db:seed:all",
    "undo-db": "npx sequelize-cli db:migrate:undo:all",
    "undo-seed": "npx sequelize-cli db:seed:undo:all",
    "prepare": "husky install",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint:report": "eslint \"src/**/*.{ts,tsx}\" --format stylish"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.2",
    "@types/pg": "^8.11.11",
    "@types/sequelize": "^4.28.20",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "xlsx": "^0.18.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
